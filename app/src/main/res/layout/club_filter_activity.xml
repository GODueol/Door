<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#616161">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:elevation="6dp"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light" >

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/core_icon"/>

    </android.support.v7.widget.Toolbar>

    <!--<ScrollView-->
        <!--android:layout_below="@+id/toolbar"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:layout_alignParentLeft="true"-->
        <!--android:layout_alignParentStart="true">-->

    <TextView
        android:id="@+id/txt_save"
        android:layout_below="@+id/toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginRight="15dp"
        android:layout_alignParentRight="true"
        android:text="Save"
        android:textColor="@color/text_white"
        android:textStyle="bold"
        android:textSize="20dp"
        />

    <View
        android:id="@+id/section_line1"
        android:layout_below="@id/txt_save"
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="#FFFFFF"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        ></View>

    <TextView
        android:id="@+id/location_nouse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/section_line1"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:text="Location"
        android:textColor="@color/text_white"
        android:textStyle="bold"
        />

    <LinearLayout
        android:id="@+id/location_layout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/location_nouse"
        android:orientation="horizontal"
        android:weightSum="4"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/checkBox1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="서울"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="경기"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="강원"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="충청"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

    </LinearLayout>

    <LinearLayout
            android:id="@+id/location_layout2"
            android:layout_below="@+id/location_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            android:layout_marginLeft="13dp"
            android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/checkBox5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:layout_weight="1"
            android:text="전라"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:layout_weight="1"
            android:text="경상"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:layout_weight="1"
            android:text="제주"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/checkBox8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:layout_weight="1"
            android:text="해외"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        </LinearLayout>

    <View
        android:id="@+id/section_line2"
        android:layout_below="@id/location_layout2"
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="#FFFFFF"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        ></View>

    <TextView
        android:id="@+id/age_nouse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/section_line2"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:text="Age"
        android:textColor="@color/text_white"
        android:textStyle="bold"
        />

    <LinearLayout
        android:id="@+id/age_layout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/age_nouse"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:weightSum="5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/age_checkBox1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="10대"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/age_checkBox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="20대"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/age_checkBox3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="30대"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/age_checkBox4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="40대"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/age_checkBox5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="50대"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />
    </LinearLayout>

    <View
        android:id="@+id/section_line3"
        android:layout_below="@id/age_layout3"
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:background="#FFFFFF"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        ></View>

    <TextView
        android:id="@+id/theme_nouse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/section_line3"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:text="Theme"
        android:textColor="@color/text_white"
        android:textStyle="bold"
        />

    <LinearLayout
        android:id="@+id/theme_layout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/theme_nouse"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:weightSum="5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/theme_checkBox1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="운동"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="게임"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="만남"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="취향"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="공부"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/theme_layout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/theme_layout4"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:weightSum="5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/theme_checkBox6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="친목"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="동갑"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="예술"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="기술"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="여행"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/theme_layout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/theme_layout5"
        android:orientation="horizontal"
        android:layout_centerHorizontal="true"
        android:weightSum="5"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        >

        <CheckBox
            android:id="@+id/theme_checkBox11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="학교"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="음식"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="종교"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="동물"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
            android:onClick="androidCheckBoxClicked"
            />

        <CheckBox
            android:id="@+id/theme_checkBox15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:checked="true"
            android:text="기타"
            android:textColor="@color/text_white"
            android:theme="@style/MyCheckBox"
